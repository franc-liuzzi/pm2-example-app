{
  "name": "pm2-example-app",
  "version": "1.0.0",
  "description": "",
  "main": "dist/index.js",
  "precommit": "lint",
  "scripts": {
    "build": "run-s build:clean build:build",
    "build:clean": "rimraf dist",
    "build:build": "babel src --out-dir dist",
    "build:dev": "babel src --watch --out-dir dist",
    "start": "run-s build start:pm2",
    "start-dev": "tmex -n build:dev start:pm2",
    "start:pm2": "pm2 start ecosystem.config.js",
    "stop": "pm2 delete all",
    "lint": "eslint --fix src"
  },
  "keywords": ["pm2", "node", "express", "babel", "eslint"],
  "author": "Francesco Liuzzi",
  "license": "ISC",
  "dependencies": {
    "express": "^4.17.1",
    "http": "0.0.1-security"
  },
  "devDependencies": {
    "@babel/cli": "^7.8.4",
    "@babel/core": "^7.9.6",
    "@babel/node": "^7.8.7",
    "@babel/preset-env": "^7.9.6",
    "@babel/register": "^7.9.0",
    "core-js": "^3.6.5",
    "eslint": "^6.8.0",
    "eslint-config-airbnb-base": "^14.1.0",
    "eslint-plugin-import": "^2.20.2",
    "npm-run-all": "^4.1.5",
    "pm2": "^4.4.0",
    "pre-commit": "^1.2.2",
    "rimraf": "^3.0.2",
    "tmex": "^1.0.5"
  }
}
