    "version": "0.2.0",
    "configurations": [
        {
            "type": "node",
            "runtimeExecutable": "${workspaceFolder}/node_modules/.bin/babel-node",
            "runtimeArgs": [
              "--nolazy"
            ],
            "request": "launch",
            "name": "Launch pm2-example-app",
            "skipFiles": [
                "<node_internals>/**"
            ],
            "program": "${workspaceFolder}/src/index.js",
            "env": {
              "BABEL_ENV": "debug"
            },
        }
    ]
}